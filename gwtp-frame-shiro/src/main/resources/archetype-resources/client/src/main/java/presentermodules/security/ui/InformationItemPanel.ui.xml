<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui'
	xmlns:m='urn:import:gwt.material.design.client.ui'>

<ui:style gss="true">
.infoPice {
width:             100%;
cursor:            pointer;
}

.infoPice:hover .separator,  .infoPice:hover+.infoPice .separator {
padding-left: 0;
}

.anchorStyle {
will-change: transform,opacity;
position: relative;
outline: none;
overflow: hidden;
}

.separator {
padding-left: 24px;
transition: padding-left 15ms ease-in-out;
}

.separatorBorder {
border-top: 1px solid #dadce0;
}

.anchorStyle {
padding-top: 15px;
padding-bottom: 16px;
padding-left: 24px;
padding-right: 24px;
height: 100%;
color: #1a73e8;
text-decoration: none;
display: block;
}

.anchorStyle:hover {
background-color: rgba(0,0,0,.04);
}

.anchorStyleInside {
-webkit-box-align: center;
box-align: center;
-webkit-align-items: center;
align-items: center;
display: -webkit-box;
display: -moz-box;
display: -ms-flexbox;
display: -webkit-flex;
display: flex;
}

.anchorFlex {
-webkit-box-align: center;
box-align: center;
-webkit-align-items: center;
align-items: center;
display: -webkit-box;
display: -moz-box;
display: -ms-flexbox;
display: -webkit-flex;
display: flex;
}

.anchorKey {
-webkit-flex-basis: 156px;
flex-basis: 156px;
margin-right: 24px;
padding-top: 4px;
-webkit-box-align: center;
box-align: center;
-webkit-align-items: center;
align-items: center;
display: -webkit-box;
display: -moz-box;
display: -ms-flexbox;
display: -webkit-flex;
display: flex;
}

.anchorKeyH {
letter-spacing: .07272727em;
font-size: .7rem;
font-weight: 500;
line-height: 1rem;
text-transform: uppercase;
-webkit-hyphens: auto;
hyphens: auto;
word-break: break-word;
word-wrap: break-word;
color: #5f6368;
margin: 0;
padding: 0;
width: 100%;
}

.anchorValue {
-webkit-box-flex: 1;
box-flex: 1;
-webkit-flex-grow: 1;
flex-grow: 1;
-webkit-flex-shrink: 1;
flex-shrink: 1;
margin-right: 24px;
-webkit-flex-basis:328px;
-ms-flex-preferred-size:328px;
flex-basis:328px;
-webkit-box-align: center;
box-align: center;
-webkit-align-items: center;
align-items: center;
display: -webkit-box;
display: -moz-box;
display: -ms-flexbox;
display: -webkit-flex;
display: flex;
padding-top: 4px;
}

.anchorValueH {
letter-spacing: .00625em;
font-size: 1rem;
font-weight: 400;
line-height: 1.5rem;
-webkit-hyphens: auto;
hyphens: auto;
word-break: break-word;
word-wrap: break-word;
color: #202124;
margin: 0;
padding: 0;
width: 100%;
}

.anchorArrow {
-webkit-box-align: center;
box-align: center;
-webkit-align-items: center;
align-items: center;
-webkit-box-pack: center;
box-pack: center;
-webkit-justify-content: center;
justify-content: center;
height: 24px;
margin: 0;
padding: 0;
width: 24px;
fill:  #202124;
}

.anchorKeyValue {
-webkit-box-flex: 1;
box-flex: 1;
-webkit-flex-grow: 1;
flex-grow: 1;
-webkit-flex-shrink: 1;
flex-shrink: 1;
-webkit-flex-basis: 0;
flex-basis: 0;

-webkit-box-align: stretch;
box-align: stretch;
-webkit-align-items: stretch;
align-items: stretch;
display: -webkit-box;
display: -moz-box;
display: -ms-flexbox;
display: -webkit-flex;
display: flex;
-webkit-flex-wrap: wrap;
flex-wrap: wrap;    
}
</ui:style>

<g:HTMLPanel styleName="{style.infoPice}">
 <div class="{style.separator}">
  <div class="{style.separatorBorder}"/>
 </div>
 <div class="{style.anchorStyle}">
  <div class="{style.anchorStyleInside}">
   <div class="{style.anchorKeyValue}">
    <div class="{style.anchorKey}">
     <div class="{style.anchorKeyH}" ui:field="key"/>
    </div>
    <div class="{style.anchorValue}">
     <div class="{style.anchorValueH}" ui:field="value"/>
    </div>
   </div>
   <figure class="{style.anchorArrow}" ui:field="link">
	<m:MaterialIcon iconType="FORWARD" />
   </figure>
  </div>
 </div>
</g:HTMLPanel>
</ui:UiBinder>